    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.3",
    "cli-ux": "^5.6.3",
    "debug": "^4.3.2",
    "sinon": "^11.1.2",
    "tslib": "^1.14.1"
  }
